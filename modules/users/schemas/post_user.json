{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1,
            "not": { "type": "null" }
        },
        "last_name": {
            "type": "string",
            "minLength": 1,
            "not": { "type": "null" }
        },
        "city": {
            "type": "string",
            "minLength": 1,
            "not": { "type": "null" }
        },
        "phone": {
            "type": "number",
            "minLength": 1,
            "not": { "type": "null" }
        }
    },
    "required": [
        "name",
        "last_name",
        "city",
        "phone"
    ]
}